<odoo>
    <data>
        <record id="account_payment_form_type_list" model="ir.ui.view">
            <field name="name">account.payment.form.type.list</field>
            <field name="model">account.payment.form.type</field>
            <field name="type">list</field>
            <field name="arch" type="xml">
                <list string="Payment Form Type">
                    <field name="name"/>
                    <field name="code"/>
                </list>
            </field>
        </record>

        <record id="account_payment_form_type_form" model="ir.ui.view">
            <field name="name">account.payment.form.form</field>
            <field name="model">account.payment.form.type</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Payment Form Type">
                    <header>
                    </header>
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="code"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>