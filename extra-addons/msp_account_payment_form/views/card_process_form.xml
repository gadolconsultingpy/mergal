<odoo>
    <data>
        <record id="card_process_form_list" model="ir.ui.view">
            <field name="name">card.process.form.list</field>
            <field name="model">card.process.form</field>
            <field name="type">list</field>
            <field name="arch" type="xml">
                <list string="Card Process Form">
                    <field name="name"/>
                    <field name="code"/>
                </list>
            </field>
        </record>

        <record id="card_issuer_form" model="ir.ui.view">
            <field name="name">card.process.form</field>
            <field name="model">card.process.form</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Card Process Form">
                    <header>
                    </header>
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="code"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>