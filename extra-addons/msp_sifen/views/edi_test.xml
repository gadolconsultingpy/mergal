<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_edi_test_list" model="ir.ui.view">
        <field name="name">edi.test.tree</field>
        <field name="model">edi.test</field>
        <field name="arch" type="xml">
            <list string="EDI Test">
                <field name="name"/>
                <field name="description"/>
            </list>
        </field>
    </record>

    <record id="view_edi_test_form" model="ir.ui.view">
        <field name="name">edi.test.form</field>
        <field name="model">edi.test</field>
        <field name="arch" type="xml">
            <form string="EDI Test">
                <header>
                    <button name="action_apply_test" string="Apply Test" type="object" class="oe_highlight"/>
                </header>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="name"/>
                        </h1>
                    </div>
                    <group>
                        <group>
                            <field name="description"/>
                            <field name="security_code_1"/>
                            <field name="security_code_2"/>
                            <field name="branch_id"/>
                            <field name="establishment"/>
                            <field name="dispatch_point"/>
                            <field name="stamped_number"/>
                            <field name="from_date"/>
                        </group>
                        <group>
                            <field name="company_id"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>