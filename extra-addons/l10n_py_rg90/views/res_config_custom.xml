<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_custom_form" model="ir.ui.view">
            <field name="name">>res.config.custom.form</field>
            <field name="model">res.config.custom</field>
            <field name="inherit_id" ref="l10n_py_base.res_config_custom_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page name="rg90_page" string="RG90/2021">
                        <group>
                            <group name="group_left" string="Document Types">
                                <field name="out_invoice_doc_type_id"/>
                                <field name="out_refund_doc_type_id"/>
                                <field name="in_invoice_doc_type_id"/>
                                <field name="in_refund_doc_type_id"/>
                            </group>
                            <group name="group_right" string="Sales Taxes">
                            </group>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>