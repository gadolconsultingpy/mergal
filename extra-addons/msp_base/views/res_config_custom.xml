<odoo>
    <record id="res_config_custom_list" model="ir.ui.view">
        <field name="name">res.config.custom.list</field>
        <field name="model">res.config.custom</field>
        <field name="arch" type="xml">
            <list string="Additional Parameters">
                <field name="name"/>
                <field name="company_id"/>
            </list>
        </field>
    </record>

    <record id="res_config_custom_form" model="ir.ui.view">
        <field name="name">Additional Parameters Form</field>
        <field name="model">res.config.custom</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Additional Parameters">
                <header>

                </header>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="name"/>
                        </h1>
                    </div>
                    <group name="group1">
                        <group name="header_left">
                            <field name="company_id"/>
                        </group>
                        <group name="header_right">
                        </group>
                    </group>
                    <notebook name="notebook">
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
</odoo>