<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id='account_check_third_sequence' model='ir.sequence'>
            <field name="name">Third Checks Sequence</field>
            <field name="implementation">standard</field>
            <field name="code">account.check.third</field>
            <field name="prefix">CHQT-</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
        </record>
        <record id='account_check_own_sequence' model='ir.sequence'>
            <field name="name">Own Checks Sequence</field>
            <field name="implementation">standard</field>
            <field name="code">account.check.own</field>
            <field name="prefix">CHPR-</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
        </record>
        <record id='account_check_deposit_sequence' model='ir.sequence'>
            <field name="name">Account Securities Deposit Sequence</field>
            <field name="implementation">standard</field>
            <field name="code">account.securities.deposit</field>
            <field name="prefix">DEP-</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="padding">7</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>