<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <data>
        <!-- ACCOUNT MOVE (EDI FIELDS)  -->
        <record id="action_edi_account_move" model="ir.actions.act_window">
            <field name="binding_type">action</field>
            <field name="context">{'default_move_type': 'out_invoice'}</field>
            <field name="domain">[('move_type', 'in', ['out_invoice', 'out_refund']),
                                  ('state', '=', 'posted')]</field>
            <field name="groups_id" eval="[(6, 0, [])]"/>
            <field name="help"><![CDATA[Electronic Documents]]></field>
            <field name="name">Electronic Documents</field>
            <field name="res_model">account.move</field>
            <field name="target">current</field>
            <field name="view_id" ref="edi_account_move_list"></field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">list,pivot,graph,form</field>
        </record>
        <!-- STOCK PICKING (EDI FIELDS)  -->
        <record id="action_edi_stock_picking" model="ir.actions.act_window">
            <field name="binding_type">action</field>
            <field name="context">{}</field>
            <field name="domain">[('journal_id.invoice_type_id.code', '=', '7'),
                                  ('sequence_id', '!=', False),
                                  ('state', '=', 'done')]</field>
            <field name="groups_id" eval="[(6, 0, [])]"/>
            <field name="help"><![CDATA[Electronic Documents]]></field>
            <field name="name">Electronic Documents</field>
            <field name="res_model">stock.picking</field>
            <field name="target">current</field>
            <field name="view_id" ref="edi_stock_picking_list"></field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">list,form</field>
        </record>
